{
  "name": "@opennhp/agent",
  "version": "1.0.0",
  "description": "OpenNHP JavaScript/TypeScript SDK for zero-trust network security",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "format": "prettier --write src test",
    "docs": "typedoc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nhp",
    "opennhp",
    "zero-trust",
    "network-security",
    "cryptography"
  ],
  "author": "OpenNHP",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenNHP/js-agent.git"
  },
  "dependencies": {
    "@noble/ciphers": "^2.1.1",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^1.7.2",
    "sm-crypto-v2": "^1.15.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.15.21",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "typedoc": "^0.28.15",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.0"
  }
}
